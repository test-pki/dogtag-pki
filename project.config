[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group test-pki-approvers
	owner = group user/vakwetu
[access "refs/for/refs/*"]
	read = group test-pki-approvers
	read = group user/vakwetu
	push = group test-pki-approvers
	push = group user/vakwetu
	label-Code-Review = -2..+2 group test-pki-approvers
	label-Code-Review = -2..+2 group user/vakwetu
	label-Verified = -1..+1 group test-pki-approvers
	label-Verified = -1..+1 group user/vakwetu
	label-Workflow = -1..+1 group test-pki-approvers
	label-Workflow = -1..+1 group user/vakwetu
	rebase = group test-pki-approvers
	rebase = group user/vakwetu
	removeReviewer = group test-pki-approvers
	removeReviewer = group user/vakwetu
	submit = group test-pki-approvers
	submit = group user/vakwetu
[access "refs/heads/*"]
	read = group test-pki-approvers
	read = group user/vakwetu
	create = group test-pki-approvers
	create = group user/vakwetu
	push = group test-pki-approvers
	push = +force group user/vakwetu
	pushMerge = group test-pki-approvers
	pushMerge = group user/vakwetu
[access "refs/tags/*"]
	push = group test-pki-approvers
	push = group user/vakwetu
	push = +force group user/vakwetu
	label-Code-Review = -2..+2 group test-pki-approvers
	label-Verified = -1..+1 group test-pki-approvers
	label-Workflow = -1..+1 group test-pki-approvers
	label-Workflow = -1..+1 group user/vakwetu
	submit = group test-pki-approvers
[access "refs/meta/*"]
	owner = group test-pki-approvers
	submit = group test-pki-approvers
	label-Verified = -1..+1 group test-pki-approvers
	label-Workflow = -1..+1 group test-pki-approvers
	label-Workflow = -1..+1 group user/vakwetu
[label "Workflow"]
	function = MaxWithBlock
	value = -1 Work in Progress
	value =  0 Ready for Reviews
	value = +1 Ready to Merge
